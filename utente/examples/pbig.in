process big8 body big_c(8), 10, LIV_UTENTE;
process big9 body big_c(9), 10, LIV_UTENTE;

#define N  9000000
unsigned int values[N];

process_body big_c(int a)
{
	printf(0, "scrivo i valori\n");
	for (int i = 0; i < N; i++) {
		values[i] = i;
		if (i % 65536 == 0) printf(0, "%d", a);
	}
	printf(0, "\nora li sommo\n");
	unsigned int sum = 0;
	for (int i = 0; i < N; i++) {
		sum += values[i];
		if (i % 65536 == 0) printf(0, "%d", a);
	}
	printf(0, "\nsomma: %d\n", sum);
}

