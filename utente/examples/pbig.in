/*
 * Programma Big 
 */

#include <sys.h>
#include <lib.h>

process big body big_c(0), 10, LIV_UTENTE;
process big2 body big_c(0), 10, LIV_UTENTE;

#define N  9000000
unsigned int values[N];

process_body big_c(int a)
{
	printf(a, "scrivo i valori\n");
	for (int i = 0; i < N; i++) {
		values[i] = i;
		if (i % 4096 == 0) printf(a, ".");
	}
	printf(a, "\nora li sommo\n");
	unsigned int sum = 0;
	for (int i = 0; i < N; i++) {
		sum += values[i];
		if (i % 4096 == 0) printf(a, ".");
	}
	printf(a, "\nsomma: %d\n", sum);
}

